{
  "name": "gpfm-api",
  "version": "1.0.0",
  "engines": {
    "node": "14.3.0",
    "npm": "6.14.5"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index",
    "dev": "cross-env NODE_ENV=development nodemon --inspect",
    "lint": "cross-env NODE_ENV=development eslint ./**/*.js",
    "test": "cross-env NODE_ENV=test echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize db:create",
    "db:drop": "npx sequelize db:drop",
    "db:migrate": "npx sequelize db:migrate",
    "db:status": "npx sequelize db:migrate:status",
    "db:seed": "npx sequelize db:seed:all",
    "db:restart": "NODE_ENV=development npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed && npm run dev",
    "db:rollback": "npx sequelize db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.21.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pdfmake": "^0.1.71",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.10",
    "uuid": "^8.1.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1"
  }
}
